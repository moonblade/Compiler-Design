#include<iostream.h>
int main()
{
	int first=2;
	cout<<"Enter First Number : ";
	cin>>first;
	cout<<"Enter Second Number : ";
	int second;
	cin>>second;
	cout<<"Enter Third Number : ";
	int third;
	cin>>third;
	int fourth;
	cout<<"Result is (first+second)*third : ";
	fourth=(first+second)*third;
	cout<<fourth;
}
